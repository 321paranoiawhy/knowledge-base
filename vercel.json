{
  "framework": "vitepress",
  "installCommand": "pnpm i",
  "buildCommand": "pnpm docs:build",
  "ignoreCommand": "if [ '$VERCEL_ENV' == 'production' && '$VERCEL_GIT_COMMIT_REF' == 'main' ]; then exit 1; else exit 0; fi",
  "cleanUrls": true,
  "public": false,
  "outputDirectory": ".vitepress/dist",
  "github": {
    "silent": true
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "gh-pages": false
    }
  }
}
